{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["post"],
      "route": "data"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "outBlob",
      "type": "blob",
      "path": "master-thesis-data/{game}/{user}/{uuid}.json",
      "connection": "StorageConnection",
      "direction": "out"
    }
  ]
}